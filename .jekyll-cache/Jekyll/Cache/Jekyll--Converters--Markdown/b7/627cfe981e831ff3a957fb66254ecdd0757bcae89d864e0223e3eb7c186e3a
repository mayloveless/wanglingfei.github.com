    if (a === 0) return b;
    if (b === 0) return a;
    const res = a ^ b;// æ— è¿›ä½ç›¸åŠ 
    return twoSum(res, (a &amp; b) &lt;&lt; 1); //ç»“æœ åŠ ä¸Š è¿›ä½
}
</code></pre></div></div>

<p>46ã€å®ç°curry</p>

<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>function currying(fn) {
    const argArr = [];
    let closure = function(...args) {
        if(args.length &gt; 0) {
            argArr = [...argArr, ...args];
            return closure;
        }
        return fn(...argArr);
    }
    return closure;
}
</code></pre></div></div>

<p>47ã€<a href="https://github.com/ConardLi/ConardLi.github.io/blob/master/demo/deepClone/src/clone_6.js">å®ç°æ·±æ‹·è´</a></p>

<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>function deepCopy(o) {
    if (typeof o !== "object") return o;
    let n;
    if (Array.isArray(o)) {
        n = new Array(o.length);
        o.forEach((v,i) =&gt; (n[i] = deepCopy(v)));
    }

    // reg math function ç­‰å…¶ä»–ç±»å‹æš‚æ—¶ä¸è€ƒè™‘
    else if (!Array.isArray(o)) {
        n = {};
        Object.keys(o).forEach(key =&gt; {
            n[key] = deepCopy(o[key]);
        });
    }
     return n;
}
</code></pre></div></div>

