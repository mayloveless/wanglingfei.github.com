    return tpl.replace(/\{\{(.+?)\}\}/g, function($1, $2) {
        // $1 åˆ†ç»„ä¸º ç±»ä¼¼ 
        // $2 åˆ†ç»„ä¸º ç±»ä¼¼ name
        // åŠ ä¸Šé¢çš„å°æ‹¬å·å°±æ˜¯ä¸ºäº†æ–¹ä¾¿æ‹¿åˆ°keyè€Œå·²
        return data[$2];
