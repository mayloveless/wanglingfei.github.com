<p>43ã€å¤§æ•°ç›¸åŠ </p>

<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>function bigNumberSum(a, b) {
    // 123456789  000009876
    // padding
    let cur = 0;
    while (cur &lt; a.length || cur &lt; b.length) {
        if (!a[cur]) {
            a = "0" + a;
        } else if (!b[cur]) {
            b = "0" + b;
        }
        cur++;
    }

    let carried = 0;
    const res = [];

    for (let i = a.length - 1; i &gt; -1; i--) {
        const sum = carried + +a[i] + +b[i];
        if (sum &gt; 9) {
            carried = 1;
        } else {
            carried = 0;
        }
        res[i] = sum % 10;
    }
    if (carried === 1) {
        res.unshift(1);
    }
    return res.join("");
}
</code></pre></div></div>

<p>44ã€æ‰‹å†™ bind</p>

