/*  é€’å½’è§£æ³•
    if( n === 1){
        return 1;
    }
    if( n === 2 ){
        return 2;
    }
    return climbStairs(n - 2) + climbStairs(n-1) */
// å¾ªç¯è§£æ³•
    if (n === 1) {
        return 1;
    }
    if (n === 2) {
        return 2;
    }
    let prev = 1;
    let next = 2;
    let res = 0;
    for(var i=2;i&lt;n;i++){
        res = prev + next;// å¯¹åº”é€’å½’è§£æ³•
        prev = next;
        next = res;
    }
    return res;
