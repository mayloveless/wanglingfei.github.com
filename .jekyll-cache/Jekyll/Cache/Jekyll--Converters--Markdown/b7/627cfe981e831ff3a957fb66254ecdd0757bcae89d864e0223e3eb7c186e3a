<p>17ã€ä¸‰æ•°ä¹‹å’Œ</p>

<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>var threeSum = function(nums) {
    let res = [];
    nums.sort((a,b)=&gt;{
        return a - b;
    });// æ’åº
    length = nums.length;
    if (nums[0] &lt;= 0 &amp;&amp; nums[length - 1] &gt;= 0) { //æ»¡è¶³å‰é¢å°äº0ï¼Œåé¢å¤§äº0ï¼›
        for(var i=0;i&lt; length -2; ){
            if (nums[i] &gt; 0) break; // å¤§äº0äº†ï¼Œåé¢å°±ä¸ç”¨çœ‹äº†ã€‚
            
            let first = i + 1
            let last = length - 1;
            do {
                
                let result = nums[i] + nums[first] + nums[last] //æ‹¿å‡ºä¸€ä¸ªæ•°ï¼ŒæŠŠåé¢çš„æœ€å‰å’Œæœ€ååŠ ä¸€å—
                if (result === 0) { 
                    res.push([nums[i], nums[first], nums[last]]) //æ­£å¥½åˆ™pushè¿›æ•°ç»„
                }
                if (result &lt;= 0) { //å°äº†ï¼Œä¸æ–­æ‰¾åˆ°ä¸ä¸€æ ·çš„é‚£ä¸ªæ•°ï¼Œæ¥ç€éå†
                    while (first &lt; last &amp;&amp; nums[first] === nums[++first]) { }
                } else {  //å¤§äº†ï¼Œä¸æ–­æ‰¾åˆ°ä¸ä¸€æ ·çš„é‚£ä¸ªæ•°ï¼Œæ¥ç€éå†
                    while (first &lt; last &amp;&amp; nums[last] === nums[--last]) { }
                }
            } while (first &lt; last)
            while (nums[i] === nums[++i]) { } // ç¬¬ä¸€ä¸ªæ•°ä¹Ÿæ˜¯ï¼Œä¸æ–­æ‰¾åˆ°ä¸ä¸€æ ·çš„æ•°å†åˆ¤æ–­
        }
    }
    return res;
};
