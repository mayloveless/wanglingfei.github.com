var count = function (arr,nums,res) {
    if(arr.length === nums.length){ //é€’å½’åœæ­¢å¤„ï¼šå¦‚æœé•¿åº¦okäº†ï¼Œæ‰“å°æ•°æ®
        res.push(arr.slice())
        return
    }
    for (let i = 0; i &lt; nums.length; i++) {// loop å¼€å§‹
        if (arr.indexOf(nums[i]) !== -1) continue;// é‡å¤æ•°å­—ä¸ç®¡
        arr.push(nums[i]); //å­˜
        count(arr,nums,res);// é€’å½’çœ‹çœ‹
