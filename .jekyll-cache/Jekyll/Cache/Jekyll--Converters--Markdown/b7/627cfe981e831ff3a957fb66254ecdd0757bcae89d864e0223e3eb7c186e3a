<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>    var lengthOfLongestSubstring = function(s) {
        if (!s) { return 0; }
        let max = 0;
        let hash = {};
        for(let left=0,right=0;right&lt;s.length&amp;&amp;left&lt;s.length;){
            if ( !hash[s[right]] ){  
                hash[s[right]] = 1; //æ²¡æœ‰çš„å­—ç¬¦æ”¾è¿›æ¥
                right++; //å³è¾¹æŒ‡é’ˆåŠ ä¸€
                    max = Math.max(max, right - left); // é•¿åº¦å°±æ˜¯å·®å€¼
            }else{
