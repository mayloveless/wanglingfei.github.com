<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>//å‹æ ˆï¼Œpopï¼Œå‹å·¦èŠ‚ç‚¹ï¼Œå‹å³èŠ‚ç‚¹ã€‚è®°å½•æ¯ä¸ªèŠ‚ç‚¹æ·±åº¦ã€‚
var maxDepth = function(root) {
    let arr = [];
    let max = 0;
    arr.push({
        node:root,
        depth:1
    });
    while (arr.length) {
        let item = arr.pop();
        if( !item.node){
            continue
        }
        if (!item.node.left &amp;&amp; !item.node.right){
            max = max &gt; item.depth ? max : item.depth;
        }
        item.node.left &amp;&amp; arr.push({
            node: item.node.left,
            depth: item.depth+1
        })
        item.node.right &amp;&amp; arr.push({
            node: item.node.right,
            depth: item.depth+1
        })
    }
    return max
};
</code></pre></div></div>

